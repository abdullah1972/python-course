number = int(input("Type a number to check if it a primary   : "))

if number > 1:

    for increament in range(2, number):
        if (number % increament) == 0:
            print(number, "is not a prime number")
            break
    else:
        print(number, "is a prime number")

else:
    print(number, "is not a prime number")
